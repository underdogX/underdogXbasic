	script.Parent.MouseButton1Click:Connect(function()
		warn("THIS MIGHT GET A LITTLE LAGGY")
		wait(5)
		while true do
			warn("WE DO A LITTLE TROLLING")
			warn("THE SCRIPT INCIDENT")
		wait()
		end	
	end)
end
