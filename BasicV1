local function SOQDQ_fake_script() -- TextLabel_3.Changing text 
	local script = Instance.new('Script', TextLabel_3)

	while true do
	script.Parent.Text = "Loading: 5 seconds left"
	wait(1)
	script.Parent.Text = "Loading: 4 seconds left"
	wait(1)
	script.Parent.Text = "Loading: 3 seconds left"
	wait(1)
	script.Parent.Text = "Loading: 2 seconds left"
	wait(1)
	script.Parent.Text = "Loading: 1 seconds left"
	wait(1)
	end 
end
coroutine.wrap(SOQDQ_fake_script)()
